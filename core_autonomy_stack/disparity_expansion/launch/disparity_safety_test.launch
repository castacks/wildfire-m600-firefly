<!-- -->
<launch>
    <param name="/oa/baseline" 				type="double" value="0.10" />
    <param name="/oa/downsample_scale" 			type="double" value="2.0" />
    <param name="/oa/low_occ_thresh"			type="double" value="3.9" />
    <param name="/oa/high_occ_thresh"			type="double" value="1.8" />
    <param name="/oa/sensor_pixel_error"		type="double" value="0.0" />

    <node pkg="disparity_expansion" type="disparity_expansion_clean" name="disparity_expansion">
        <param name="lut_max_disparity" type="int" value="180" />
        <param name="robot_radius" type="double" value="1.5" />
        <param name="bg_multiplier" type="double" value="2.0" />
	<param name="padding" type="double" value="-1.0" />
        <param name="sensor_pixel_error" type="double" value="0.5" />
    </node>
</launch>
